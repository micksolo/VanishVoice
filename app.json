{
  "expo": {
    "name": "VanishVoice",
    "slug": "VanishVoice",
    "owner": "generictech",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "vanishvoice",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.generictech.vanishvoice",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "VanishVoice needs access to your microphone to record voice messages.",
        "NSCameraUsageDescription": "VanishVoice needs access to your camera to record video messages.",
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "vanishvoice",
            "CFBundleURLSchemes": [
              "vanishvoice",
              "exp+vanishvoice"
            ]
          }
        ]
      },
      "entitlements": {
        "aps-environment": "development"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.generictech.vanishvoice",
      "permissions": [
        "RECORD_AUDIO",
        "CAMERA"
      ]
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#000000",
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} new messages"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1c797ee2-fc1f-4b6d-bc64-15f675b57367"
      }
    }
  }
}
